{
  "name": "Prana",
  "version": "1.0.0",
  "description": "Sync your breath",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -p tinyify",
    "test": "browserify test/index.js | node | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ninja-nanz/Prana"
  },
  "author": "ninja-nanz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ninja-nanz/Prana/issues"
  },
  "homepage": "https://github.com/ninja-nanz/Prana",
  "dependencies": {
    "@psychobolt/react-paperjs": "0.0.55",
    "paper": "^0.12.0",
    "radium": "^0.25.1",
    "react-pose": "^4.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "budo": "^11.6.1",
    "gh-pages": "^0.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "tinyify": "^2.5.0",
    "watchify": "^3.7.0"
  }
}
